{
  "id": "blog",
  "label": "Blog",
  "repeatable": true,
  "status": true,
  "format": "custom",
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "slug"
        }
      },
      "external_resource_name": {
        "type": "Text",
        "config": {
          "label": "External Resource Name",
          "placeholder": ""
        }
      },
      "external_link": {
        "type": "Link",
        "config": {
          "allowTargetBlank": true,
          "label": "External Link",
          "select": null
        }
      },
      "seo_description": {
        "type": "Text",
        "config": {
          "label": "SEO Description"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "title",
          "placeholder": ""
        }
      },
      "year": {
        "type": "Number",
        "config": {
          "label": "year",
          "placeholder": ""
        }
      },
      "body": {
        "type": "StructuredText",
        "config": {
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl",
          "label": "body"
        }
      }
    }
  }
}
