{
  "id": "blog",
  "label": "Blog",
  "repeatable": true,
  "status": true,
  "format": "custom",
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "slug"
        }
      },
      "article_technologies": {
        "type": "Group",
        "config": {
          "label": "Article Technologies",
          "repeat": true,
          "fields": {
            "article_tech": {
              "type": "Select",
              "config": {
                "label": "Article Tech",
                "placeholder": "",
                "options": [
                  "Javascript",
                  "Swift",
                  "WebGL",
                  "WebGPU",
                  "Metal",
                  "HTML",
                  "CSS",
                  "SVG",
                  "Three.js",
                  "React"
                ]
              }
            }
          }
        }
      },
      "table_of_contents": {
        "type": "StructuredText",
        "config": {
          "label": "Table of Contents",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "preformatted"
        }
      },
      "external_resource_name": {
        "type": "Text",
        "config": {
          "label": "External Resource Name",
          "placeholder": ""
        }
      },
      "external_link": {
        "type": "Link",
        "config": {
          "allowTargetBlank": true,
          "label": "External Link",
          "select": null
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "title",
          "placeholder": ""
        }
      },
      "year": {
        "type": "Number",
        "config": {
          "label": "year",
          "placeholder": ""
        }
      },
      "body": {
        "type": "StructuredText",
        "config": {
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl",
          "label": "body"
        }
      }
    },
    "Metadata": {
      "metadata_description": {
        "type": "Text",
        "config": {
          "label": "Metadata Description",
          "placeholder": ""
        }
      },
      "metadata_image": {
        "type": "Image",
        "config": {
          "label": "Metadata Image",
          "constraint": {
            "width": 1200,
            "height": 628
          },
          "thumbnails": []
        }
      }
    }
  }
}
